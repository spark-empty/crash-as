                  <v:var name="DualReceiverSupport" type="BOOLEAN">
                  <!-- /** @remarks Implements DSPI08000 */ -->
                  <a:a name="DESC">
                    <a:v><![CDATA[ <html> 
                        A feature to enable to switch the set of PCS signals that are driven during the first part
                       of the frame to a different set of PCS signals during the second part fo the frame.
                        </html> ]]>
                    </a:v>
                  </a:a>
                  <a:a name="VISIBLE" type="XPath">
                  <a:tst expr="ecu:get('SpiTSBSupport')='TRUE'" />
                  </a:a> 
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantLinkTime</icc:v>
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="Freescale"/>
                  <a:a name="LOWER-MULTIPLICITY" value="1"/>
                  <a:a name="UPPER-MULTIPLICITY" value="1"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="ECUC:bf1f485a-9e32-4c55-946e-4b66a4ea5f03"/>
                  <a:da name="DEFAULT" value="false"/>
               <a:da name="ENABLE" type="XPath" expr="node:exists(../TSBModeEnable) and (node:value(../TSBModeEnable) = 'true')"/>
                </v:var>
                    <v:var name="SecondaryFrameSize" type="INTEGER">
                          <!-- /** @remarks Implements DSPI08000 */ -->
                          <a:a name="DESC">
                            <a:v><![CDATA[
                              <html>
                                The FMSZ field vaule(+1) equal the data frame bit number, where the control of PCS
                                switches from the DSPI_DSICR to DSPI_CR1 register<br>
                                    ]]>
                            </a:v>
                          </a:a>
                          <a:a name="VISIBLE" type="XPath">
                          <a:tst expr="ecu:get('SpiTSBSupport')='TRUE'" />
                          </a:a> 
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="Link">VariantLinkTime</icc:v>
                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="Freescale"/>
                          <a:a name="LOWER-MULTIPLICITY" value="1"/>
                          <a:a name="UPPER-MULTIPLICITY" value="1"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:11468f3f-bdcd-42e6-a085-ba34c20b8f67"/>
                          <a:da name="DEFAULT" value="16"/>
                          <a:da name="INVALID" type="Range">
                            <a:tst expr="&lt;=31"/>
                            <a:tst expr="&gt;=3"/>
                          </a:da>
                         <a:da name="ENABLE" type="XPath" expr="node:exists(../DualReceiverSupport) and (node:value(../DualReceiverSupport) = 'true')"/>
                        </v:var>
                       <v:var name="SecondaryDsiCsIdentifier" type="STRING">
                          <!-- /** @remarks Implements DSPI08000 */ -->
                          <a:a name="DESC">
                            <a:v><![CDATA[
                              <html>
                                This parameter is the symbolic name to identify the Chip Select (CS) allocated to this Job in DSI mode in dual receiver mode.
                              </html>
                                    ]]>
                            </a:v>
                          </a:a>
                          <a:a name="VISIBLE" type="XPath">
                          <a:tst expr="ecu:get('SpiTSBSupport')='TRUE'" />
                          </a:a> 
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="Link">VariantLinkTime</icc:v>
                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="Freescale"/>
                          <a:a name="LOWER-MULTIPLICITY" value="1"/>
                          <a:a name="UPPER-MULTIPLICITY" value="1"/>
                          <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                          <a:a name="UUID" value="ECUC:1cd017e5-5788-411b-9159-982a86c465fc"/>
                          <a:da name="DEFAULT" value="DPCS0"/>
                          <a:da name="INVALID" type="XPath">
                            <a:tst expr="(. = 'DPCS0' ) or  (. = 'DPCS1' ) or (. = 'DPCS2' ) or (. = 'DPCS3' ) or (. = 'DPCS4' ) or (. = 'DPCS5' )or (. = 'DPCS6' )or (. = 'DPCS7' )" 
                              false="Invalid name of the SpiCs.Must always begin with DPCS0 or DPCS1 or DPCS2 or DPCS3 or DPCS4 or DPCS5 or DPCS6 or DPCS7 "/>
                            </a:da>
                         <a:da name="ENABLE" type="XPath" expr="node:exists(../DualReceiverSupport) and (node:value(../DualReceiverSupport) = 'true')"/>
                        </v:var>
                    </v:ctr>
                  </v:lst>
                  <v:lst name="SpiSequence" type="MAP">
                    <!-- /** @remarks Implements DSPI01500 */ -->
                    <a:da name="MIN" value="1"/>
                    <a:da name="INVALID" type="XPath">
                    <a:tst expr="((num:i(count(../SpiSequence/*)) > 256) and (../../../SpiNonAUTOSAR/SpiAllowBigSizeCollections ='false'))" true="Value out of range:Enable 'SpiAllowBigSizeCollections' to configure more than 256 Sequences)"/>
                    </a:da>
                    <v:ctr name="SpiSequence" type="IDENTIFIABLE">
                      <a:a name="LOWER-MULTIPLICITY" value="1"/>
                      <a:a name="UPPER-MULTIPLICITY" value="*"/>
                      <a:a name="DESC">
                        <a:v><![CDATA[ <html> All data needed to configure one SPI-sequence. </html> ]]>
                        </a:v>
                      </a:a>
                      <a:a name="UUID" value="ECUC:8ef631aa-0993-4f19-85e5-3bd53d6f1a56"/>
                      <v:var name="SpiInterruptibleSequence" type="BOOLEAN">
                        <!-- /** @remarks Implements DSPI01510 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[ <html> This parameter allows or not this Sequence to be suspended by another one. </html> ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="Link">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:d1513b84-b935-47d7-9786-aa18b8d91021"/>
                        <a:da name="DEFAULT" value="false"/>                      
                        <a:da name="INVALID" type="XPath">
                          <a:tst expr="((. ='true') and ((../../../../../SpiGeneral/SpiInterruptibleSeqAllowed ='false') or ((../../../../../SpiGeneral/SpiLevelDelivered) = 0)))" 
                            true="Select SpiInterruptibleSequence only when SpiGeneral/SpiInterruptibleSeqAllowed is true."/>
                        </a:da>
                      </v:var>
                      <v:var name="SpiSeqEndNotification" type="FUNCTION-NAME">
                        <!-- /** @remarks Implements DSPI02320 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[ <html> This parameter is a reference to a notification function. </html> ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="Link">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:be2809fb-411f-4fb7-a4b6-ab172f516122"/>
                        <a:da name="DEFAULT" value="NULL_PTR"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst expr="((.='NULL') or (.='Null') or (.='false') or (.='FALSE') or (.='null') or                 (.='null_ptr') or (.='Null_Ptr'))" 
                            true="Invalid name of the SpiSeqEndNotification. Must be valid C function name  or  NULL_PTR;."/>                                                     
                          <a:tst expr="text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$')" 
                            false="Invalid name of the SpiSeqEndNotification. Must be valid C function name    or NULL_PTR;."/>                    
                        </a:da>
                        <a:da name="ENABLE" type="XPath">
                          <a:tst expr="((../../../../../SpiGeneral/SpiLevelDelivered ='1') or (../../../../../SpiGeneral/SpiLevelDelivered ='2'))"/>
                        </a:da>
                      </v:var>
                      <v:var name="SpiSequenceId" type="INTEGER">
                        <!-- /** @remarks Implements DSPI01530 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[ <html> Sequence ID of configured SPI Sequence.  
                                  ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                        <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:f676838a-a6c6-4304-9889-7a1d8df2968a"/>
                        <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index,'0')"/>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=4294967295"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                          <a:da name="RANGE" type="XPath">
                          <a:tst     expr="(node:fallback( ., 0 ) &gt;= 0) and (node:fallback( ., 0 ) &lt; num:i(count(node:fallback( node:current()/../../*, 1 ))))" 
                           false="Value out of range: must be in range 0 to N-1 (N is number of configured Sequences). Use the Calc button to calculate correct default value"/>
                           <a:tst     expr="text:uniq(node:fallback( ../../*/SpiSequenceId, text:split('0') ), node:fallback( ., 1 ))" 
                           false="Duplicated value, SpiSequenceId must be unique across all Spi Sequences. Use the Calc button to calculate correct default value."/>
                          </a:da>   
                      </v:var>
                      <v:lst name="JobAssignment">
                        <!-- /** @remarks Implements DSPI01540 */ -->
                        <a:da name="MIN" value="1"/>
                        <v:ref name="JobAssignment" type="REFERENCE">
                          <a:a name="DESC">
                            <a:v><![CDATA[ <html> A sequence references several jobs, which are executed during a communication sequence. </html> ]]>
                            </a:v>
                          </a:a>
                          <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                               type="IMPLEMENTATIONCONFIGCLASS">
                            <icc:v class="Link">VariantLinkTime</icc:v>
                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                          </a:a>
                          <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                          <a:a name="LOWER-MULTIPLICITY" value="1"/>
                          <a:a name="UPPER-MULTIPLICITY" value="*"/>
                          <a:a name="UUID" value="ECUC:f554ffa6-fd41-4982-a49f-18f58bd76576"/>
                          <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/Spi/SpiDriver/SpiJob"/>
                          <a:da name="INVALID" type="XPath">
                            <!-- /** @remarks Implements DSPI02200, DSPI02210 */ -->
                            <a:tst expr="node:islast() or (node:ref(../*[position()-2=node:current()/@index])/SpiJobPriority &lt;= node:ref(node:current())/SpiJobPriority)" 
                            false="Jobs linked in a sequence shall have decreasing priorities."/>
                          </a:da>                              
                        </v:ref>
                      </v:lst>
                    </v:ctr>
                  </v:lst>
                </v:ctr>
              </v:lst>
              <v:ctr name="SpiGeneral" type="IDENTIFIABLE">
                <!-- /** @remarks Implements DSPI01400 */ -->
                <a:a name="LOWER-MULTIPLICITY" value="1"/>
                <a:a name="UPPER-MULTIPLICITY" value="1"/>
                <a:a name="DESC">
                  <a:v><![CDATA[ <html> General configuration settings for SPI-Handler. </html> ]]>
                  </a:v>
                </a:a>
                <a:a name="UUID" value="ECUC:a48dd019-7baa-4dec-a9f3-cf2b61ebfb46"/>
                <v:var name="SpiCancelApi" type="BOOLEAN">
                    <!-- /** @remarks Implements DSPI01420 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[ <html> Switches the Spi_Cancel function ON or OFF. </html> ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:ebac0183-37b9-421c-90b5-5688cb2ce418"/>
                    <a:da name="DEFAULT" value="true"/>
                  </v:var>
                  <v:var name="SpiChannelBuffersAllowed" type="INTEGER">
                    <!-- /** @remarks Implements DSPI01411 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                          Selects the SPI Handler/Driver Channel Buffers usage allowed and delivered.
                            <h1>Note</h1>   <br>
                            0 - Only Internal Buffers (IB) are allowed <br> 
                            1 - Only External buffers (EB) are allowed <br>
                            2 - Both Internal (IB) and External (EB) buffers are allowed </h1></html>
                              ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:ea040396-01f1-4184-855e-3a4a1e4440e4"/>
                    <a:da name="DEFAULT" value="0"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&lt;=2"/>
                      <a:tst expr="&gt;=0"/>
                    </a:da>
                  </v:var>
                  <v:var name="SpiDevErrorDetect" type="BOOLEAN">
                    <!-- /** @remarks Implements DSPI01430, DSPI01431 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                          Switches the Development Error Detection and Notification ON or OFF.
                        </html>
                              ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:7343a40c-1dfe-430b-8c1d-5d991cff8334"/>
                    <a:da name="DEFAULT" value="true"/>
                  </v:var>
                  <v:var name="SpiHwStatusApi" type="BOOLEAN">
                    <!-- /** @remarks Implements DSPI01412 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                          Switches the Spi_GetHWUnitStatus function ON or OFF.
                        </html>
                              ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:bc25b266-385f-4411-acfc-f5b917e8cb12"/>
                    <a:da name="DEFAULT" value="true"/>
                  </v:var>
                  <v:var name="SpiInterruptibleSeqAllowed" type="BOOLEAN">
                    <!-- /** @remarks Implements DSPI01421 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                          Switches the Interruptible Sequences handling functionality ON or OFF.
                        </html>
                              ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:f40ef361-340c-4ba6-ae0d-2b9b24885dac"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="((. ='true') and (../SpiLevelDelivered ='0'))" 
                        true="Select SpiInterruptibleSequence only when SpiGeneral/SpiLevelDelivered is 1 or 2."/>
                    </a:da>
                  </v:var>
                  <v:var name="SpiLevelDelivered" type="INTEGER">
                    <!-- /** @remarks Implements DSPI01410 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                          Selects the SPI Handler/Driver level of scalable functionality that is available and delivered.
                          <h1>Note</h1> <br>
                          Level 0 Only Simple Synchronous Behavior <br>
                          Level 1 Basic Asynchronous Behaviour <br>
                          Level 2 Enhanced Behaviour    </h1></html>
                              ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:199e6b04-e2e0-4509-8c1e-4f6fea41a58d"/>
                    <a:da name="DEFAULT" value="2"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&lt;=2"/>
                      <a:tst expr="&gt;=0"/>
                    </a:da>
                  </v:var>
                  <v:var name="SpiVersionInfoApi" type="BOOLEAN">
                    <!-- /** @remarks Implements DSPI01413 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                          Switches the Spi_GetVersionInfo function ON or OFF.
                        </html>
                              ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:0c510742-2877-48b3-b7a4-2490cca80a6f"/>
                    <a:da name="DEFAULT" value="true"/>
                  </v:var>
                  <v:ref name="SpiClockRef" type="REFERENCE">
                      <!-- /** @remarks Implements DSPI01450 */ -->
                      <a:a name="DESC">
                        <a:v><![CDATA[
                               <html>
                               Reference to the SPI clock source configuration, which is set into the MCU driver configuration. </html>
                            ]]>    
                        </a:v>    
                      </a:a>    
                      <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="Link">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                      </a:a>
                      <a:a name="ORIGIN" value="Freescale"/>
                      <a:a name="UUID" value="ECUC:c44e98d9-cbbc-4777-bab7-cb54b29d8e86"/>
                      <a:a name="LOWER-MULTIPLICITY" value="1"/>
                      <a:a name="UPPER-MULTIPLICITY" value="1"/>
                      <a:a name="ENABLE" value="1"/>
                      <a:da name="REF" 
                              value="ASPathDataOfSchema:/AUTOSAR/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint"/>
                  </v:ref>
                  <v:var name="SpiGlobalDmaEnable" type="BOOLEAN">
                    <!-- /** @remarks Implements DSPI01460 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                          If checked, it allows using the DMA module during the transfer. 
                          For each DSPI unit a transfering method can be configured: FIFO or DMA.
                          <p>If not checked, all DSPI units will use FIFO transfering mode.
                          <h1>Note</h1>This is an implementation parameter.</h1></html>
                              ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Freescale"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:4f04ac85-82d7-446d-a608-c77cb42dae95"/>
                    <a:da name="DEFAULT" value="false"/>
                    <a:da name="ENABLE" type="XPath" expr="ecu:get('SpiDMAPresent') = 'TRUE'"/>
                  </v:var>
                  <v:var name="SpiSyncTransmitTimeout" type="INTEGER">
                    <!-- /** @remarks Implements DSPI01451 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                          Timeout value (microseconds) used by SPI_SyncTransmit function to wait for TX/RX FIFO to be emptied/filled. 
                          <h1>Note</h1>This is an implementation parameter. The precision of this value is quite low, 
                          as it depends of the manner in which the compiler generates code. </h1>
                        </html>
                        ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Freescale"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:21463953-9869-4d74-b851-8b3a4f27430c"/>
                    <a:da name="DEFAULT" value="50000"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&lt;=65535"/>
                      <a:tst expr="&gt;=1"/>
                    </a:da>
                  </v:var>      
                  <v:var name="SpiOptimizeOneJobSequences" type="BOOLEAN">
                    <!-- /** @remarks Implements DSPI01470 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                           Activates the SPI transmission optimization for the sequences having only one job.
                           At his moment, the optimization is in place only for synchronous transmissions.

                           <p><p>This option requires additional RAM for two internal buffers (for sequences having 
                           only one job, and for their linked channels), in order to cache configuration 
                           information to be used during transmission initialization.
                           <p>During Spi_Init() a caching action is performed on all sequences having only one job.
                           <h1>Note</h1>This is an implementation parameter.</h1>
                        </html>
                              ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Freescale"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:ca2e4352-f96c-4546-ab70-1a2e1b13d60d"/>
                    <a:da name="DEFAULT" value="false"/>
                  </v:var>
                  <v:var name="SpiOptimizedSeqNumber" type="INTEGER">
                    <!-- /** @remarks Implements DSPI01471 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                            The maximum number of expected sequences with one job, targeted for optimization.
                            <p>If, for a given configuration, the number of sequences having only one job exceeds 
                            this value, only first 'SpiOptimizedSeqNumber' sequences will 
                            operate in optimized transmission mode.
                            <h1>Note</h1>A value of 0 will define the buffer size for fitting all optimize-able sequences.</h1>
                        </html>
                        ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Freescale"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:8a60ab14-a731-42ea-b817-26c3d3390a08"/>
                    <a:da name="DEFAULT" value="0" />
                    <a:da name="ENABLE" type="XPath" expr="(node:value(../SpiOptimizeOneJobSequences))"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&lt;=64"/>
                      <a:tst expr="&gt;=0"/>
                    </a:da>
                  </v:var>      
                  <v:var name="SpiOptimizedChannelsNumber" type="INTEGER">
                    <!-- /** @remarks Implements DSPI01472 */ -->
                    <a:a name="DESC">
                      <a:v><![CDATA[
                        <html>
                            The maximum number of channels in expected sequences with one job, targeted for optimization.
                            <p>If, for a given configuration, the number of channels in sequences having only one job exceeds 
                            this value, the sequences will be optimized only in the limit of 'SpiOptimizedChannelsNumber' 
                            consumption.
                            <h1>Note</h1>A value of 0 will define the buffer size for fitting all optimize-able sequences.</h1>
                        </html>
                        ]]>
                      </a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PreCompile">VariantLinkTime</icc:v>
                      <icc:v class="PreCompile">VariantPreCompile</icc:v>
                      <icc:v class="PreCompile">VariantPostBuild</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="Freescale"/>
                    <a:a name="LOWER-MULTIPLICITY" value="1"/>
                    <a:a name="UPPER-MULTIPLICITY" value="1"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID" value="ECUC:efde4e0e-c439-4aa5-873d-12317d57e3ca"/>
                    <a:da name="DEFAULT" value="0" />
                    <a:da name="ENABLE" type="XPath" expr="(node:value(../SpiOptimizeOneJobSequences))"/>
                    <a:da name="INVALID" type="Range">
                      <a:tst expr="&lt;=64"/>
                      <a:tst expr="&gt;=0"/>
                    </a:da>
                  </v:var>      
                  <v:lst name="SpiPhyUnit" type="MAP">
                    <a:da name="MIN" value="1"/>
                    <a:da name="INVALID" type="XPath">
                      <a:tst expr="num:i(count(node:current()/*)) &gt; num:i(count(ecu:list(&apos;Spi.SpiGeneral.SpiPhyUnit.SpiPhyUnitMapping&apos;)))" 
                        true="Maximum DSPI units available for the selected derivative was exceeded."/>
                    </a:da>
                    <v:ctr name="SpiPhyUnit" type="IDENTIFIABLE">
                      <a:a name="DESC">
                        <a:v><![CDATA[
                          <html>
                            Logical to Physical SPI Bus mapping.  
                            <h1>Note</h1>This is an implementation specific container.</h1></html>
                                ]]>
                        </a:v>
                      </a:a>
                      <a:a name="LOWER-MULTIPLICITY" value="1"/>
                      <a:a name="UPPER-MULTIPLICITY" value="1"/>
                      <a:a name="UUID" value="ECUC:e675f1c6-2c2a-41f9-9a95-715ae8c12d66"/>
                      <v:var name="SpiPhyUnitMapping" type="ENUMERATION">
                        <!-- /** @remarks Implements DSPI01481 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[
                            <html>
                              Logical SpiHWunit to physical DSPI_[1|2] assignment. It depends on the number of units present in the chip version.
                              <h1>Note</h1>This is an implementation specific parameter.</h1></html>
                                  ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="Link">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="Freescale"/>
                        <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:fa49a779-73db-4c27-bf06-a7d196a2842c"/>
                        <a:da name="DEFAULT" type="XPath" expr="(ecu:list(&apos;Spi.SpiGeneral.SpiPhyUnit.SpiPhyUnitMapping&apos;))[position()-1=node:fallback(node:current()/../@index,'1')]">
                        </a:da>
                        <a:da name="RANGE" type="XPath" expr="ecu:list(&apos;Spi.SpiGeneral.SpiPhyUnit.SpiPhyUnitMapping&apos;)"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst expr="text:uniq(../../*/SpiPhyUnitMapping, .)" false="Duplicate physical DSPI channel"/>
                        </a:da>
                      </v:var>
                      <v:var name="SpiPhyUnitSync" type="BOOLEAN">
                        <!-- /** @remarks Implements DSPI01482 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[
                            <html>
                              Specific if this HwUnit can only do sync transfers.
                              If true then this hardware unit is dedicated for Synchronous transfers only.
                              If false then this hardware unit is dedicated for Asynchronous transfers only.
                              False is applicable only if SpiGeneral/SpiLevelDelivered is either 1 or 2
                              and true is applicable only if SpiGeneral/SpiLevelDelivered is 0.  
                              <h1>Note</h1>This is an implementation specific parameter.</h1></html>
                                  ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="PreCompile">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PreCompile">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="Freescale"/>
                        <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:a1f0ff89-8de2-4353-a252-11a960d12c86"/>
                        <a:da name="DEFAULT" value="true"/>
                        <a:da name="INVALID" type="XPath">
                          <a:tst expr="( .= 'true'  and contains(../../../../SpiGeneral/SpiLevelDelivered,'1')) or ( .= 'false'  and contains(../../../../SpiGeneral/SpiLevelDelivered,'0')) 
                            " true="Improper selection. Please check the SpiLevelDelivered parameter.
                              Level 0 Synchronous Behavior is supported
                              Level 1 Asynchronous Behaviour is supported
                              Level 2 Both Synchronous and Asynchronous Behaviour is supported
                              If true then this hardware unit is dedicated for Synchronous transfers only.
                              If false then this hardware unit is dedicated for Asynchronous transfers only."/>
                        </a:da>
                      </v:var>
                      <v:var name="SpiPhyUnitAsyncMethod" type="ENUMERATION">
                        <!-- /** @remarks Implements DSPI01483 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[
                            <html>
                              Transfer method of the periperal: DMA,PIO with FIFO.  
                              <h1>Note</h1>This is an implementation parameter.</h1></html>
                                  ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="Link">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="Freescale"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:ee79df9c-9bc1-49d8-bc73-ea855cf75299"/>
                        <a:da name="DEFAULT" value="PIO_FIFO"/>
                        <a:da name="ENABLE" type="XPath" expr="node:exists(../../../../SpiGeneral/SpiGlobalDmaEnable) and (node:value(../../../../SpiGeneral/SpiGlobalDmaEnable) = 'true') and (node:value(../SpiPhyUnitSync) = 'false')"/>
                        <a:da name="RANGE" type="XPath" expr="node:when(ecu:get('SpiDMAPresent') = 'TRUE', text:split('PIO_FIFO DMA'), 'PIO_FIFO')" />
                      </v:var>
                      <v:var name="SpiPhyTxDmaChannel" type="INTEGER">
                        <!-- /** @remarks Implements DSPI01484 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[
                            <html>
                              DSPI Master Transmit DMA Channel, used to prepare the DSPI transmission dataframes
                              starting from the TX buffer content.
                              <p>This parameter is required only if SpiPhyUnitAsyncMethod - 'DMA' is selected.
                              The default calculated value must be used.
                              <h1>Note</h1>This is an implementation specific parameter.</h1></html>
                                  ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="Link">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="Freescale"/>
                        <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:2c8d3423-5cd7-4b66-82c0-10f2e16a5ce8"/>
                        <a:da name="DEFAULT" type="XPath" expr="(text:split(ecu:get('SpiPhyTxDmaChannel'),',')[num:dectoint(substring-after(node:fallback(node:current()/../SpiPhyUnitMapping,'_1'),'_'))+0])"/>  
                        <a:da name="ENABLE" type="XPath" expr="node:exists(../SpiPhyUnitAsyncMethod) and (node:value(../SpiPhyUnitAsyncMethod) = 'DMA')"/>
                        <a:da name="RANGE" type="XPath">
                          <a:tst expr="text:uniq(node:fallback(../../*[SpiPhyUnitAsyncMethod='DMA']/SpiPhyTxDmaChannel,text:range(0,31,1)),node:fallback(., 1))" false="Duplicate DMA channel. Please select another channel."/>
                          <a:tst expr="node:fallback(.,0) = (text:split(ecu:get('SpiPhyTxDmaChannel'),',')[num:dectoint(substring-after(node:fallback(node:fallback(node:current()/../SpiPhyUnitMapping,'DSPI_1'),'_1'),'_'))+0])" false="Bad DMA channel value; the default calculated value must be used."/>
                        </a:da>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=31"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                      </v:var>
                      <v:var name="SpiPhyTxDmaChannelAux" type="INTEGER">
                        <!-- /** @remarks Implements DSPI01485 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[
                            <html>
                              DSPI Auxiliary Transmit DMA Channel, used to place dataframes into the DSPI registers.
                              <p>This parameter is required only if SpiPhyUnitAsyncMethod - 'DMA' is selected.
                              Please select a unique channel value between 0 to 31.  
                              <h1>Note</h1>This is an implementation specific parameter.</h1></html>
                                  ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="Link">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="Freescale"/>
                        <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:f5ddcb41-f573-4c73-8975-2184d2dd9d9f"/>
                        <a:da name="DEFAULT" type="XPath" expr="(text:split(ecu:get('SpiPhyTxDmaChannelAux'),',')[num:dectoint(substring-after(node:fallback(node:current()/../SpiPhyUnitMapping,'_1'),'_'))+0])"/>  
                        <a:da name="ENABLE" type="XPath" expr="node:exists(../SpiPhyUnitAsyncMethod) and (node:value(../SpiPhyUnitAsyncMethod) = 'DMA')"/>
                        <a:da name="RANGE" type="XPath">
                          <a:tst expr="text:uniq(node:fallback(../../*[SpiPhyUnitAsyncMethod='DMA']/SpiPhyTxDmaChannelAux,text:range(0,31,1)),node:fallback(., 1))" false="Duplicate DMA channel. Please select another channel."/>
                        </a:da>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=31"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                      </v:var>
                      <v:var name="SpiPhyRxDmaChannel" type="INTEGER">
                        <!-- /** @remarks Implements DSPI01486 */ -->
                        <a:a name="DESC">
                          <a:v><![CDATA[
                            <html>
                              DSPI Receive DMA Channel, used to read the deserialized dataframes into the RX buffers.
                              <p>This parameter is required only if SpiPhyUnitAsyncMethod - 'DMA' is selected.
                              The default calculated value must be used.  
                              <h1>Note</h1>This is an implementation specific parameter.</h1></html>
                                  ]]>
                          </a:v>
                        </a:a>
                        <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                             type="IMPLEMENTATIONCONFIGCLASS">
                          <icc:v class="Link">VariantLinkTime</icc:v>
                          <icc:v class="PreCompile">VariantPreCompile</icc:v>
                          <icc:v class="PostBuild">VariantPostBuild</icc:v>
                        </a:a>
                        <a:a name="ORIGIN" value="Freescale"/>
                        <a:a name="LOWER-MULTIPLICITY" value="1"/>
                        <a:a name="UPPER-MULTIPLICITY" value="1"/>
                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                        <a:a name="UUID" value="ECUC:9a63f4e1-8339-47f6-b9ea-41b6e7d49744"/>
                        <a:da name="DEFAULT" type="XPath" expr="(text:split(ecu:get('SpiPhyRxDmaChannel'),',')[num:dectoint(substring-after(node:fallback(node:current()/../SpiPhyUnitMapping,'_1'),'_'))+0])"/>  
                        <a:da name="ENABLE" type="XPath" expr="node:exists(../SpiPhyUnitAsyncMethod) and (node:value(../SpiPhyUnitAsyncMethod) = 'DMA')"/>
                        <a:da name="RANGE" type="XPath">
                            <a:tst expr="text:uniq(node:fallback(../../*[SpiPhyUnitAsyncMethod='DMA']/SpiPhyRxDmaChannel,text:range(0,63,1)),node:fallback(., 1))" false="Duplicate DMA channel. Please select another channel."/>
                          <a:tst expr="node:fallback(.,0) = (text:split(ecu:get('SpiPhyRxDmaChannel'),',')[num:dectoint(substring-after(node:fallback(node:current()/../SpiPhyUnitMapping,'_1'),'_'))+0])" false="Bad DMA channel value; the default calculated value must be used."/>
                        </a:da>
                        <a:da name="INVALID" type="Range">
                          <a:tst expr="&lt;=31"/>
                          <a:tst expr="&gt;=0"/>
                        </a:da>
                    </v:var>
                  </v:ctr>
                </v:lst>
              </v:ctr>
              <v:ctr name="SpiNonAUTOSAR" type="IDENTIFIABLE">
                <a:a name="LOWER-MULTIPLICITY" value="1"/>
                <a:a name="UPPER-MULTIPLICITY" value="1"/>
                <a:a name="DESC">
                  <a:v><![CDATA[ <html> 
                        Enabling the settings of this section will configure the driver in a mode not compliant with AUTOSAR requirements. 
                        </html> ]]>
                  </a:v>
                </a:a>
                <a:a name="UUID" value="ECUC:5b39880e-550a-43e2-a3f3-86ef8baf1d62"/>
                <v:var name="SpiEnableMultiSyncTransmit" type="BOOLEAN">
                  <!-- /** @remarks Covers PR-MCAL-3172 */ -->
                  <!-- /** @remarks Implements DSPI01310 */ -->
                  <a:a name="DESC">
                    <a:v><![CDATA[ <html> 
                        Allow simultaneous calls to Spi_SyncTransmit() for different threads. 
                        Two concurrent calls to Spi_SyncTransmit() will be allowed only if the related sequences do not share HW units.
                        <h1>Note</h1> Enabling this option will violate the following requirements: SPI135, SPI114.<br>
                        </html> ]]>
                    </a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantLinkTime</icc:v>
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="Freescale"/>
                  <a:a name="LOWER-MULTIPLICITY" value="1"/>
                  <a:a name="UPPER-MULTIPLICITY" value="1"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="ECUC:440825e1-3ea4-4f20-b0a1-814b5584b37c"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>     
                <v:var name="SpiAllowBigSizeCollections" type="BOOLEAN">
                  <!-- /** @remarks Covers PR-MCAL-3172 */ -->
                  <!-- /** @remarks Implements DSPI01310 */ -->
                  <a:a name="DESC">
                    <a:v><![CDATA[ <html> 
                        A feature to allow more than 256 sequences, jobs, and channels.
                        <h1>Note</h1> Enabling this option will violate the following requirements: SPI166, SPI167, SPI168.<br>
                        </html> ]]>
                    </a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantLinkTime</icc:v>
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="Freescale"/>
                  <a:a name="LOWER-MULTIPLICITY" value="1"/>
                  <a:a name="UPPER-MULTIPLICITY" value="1"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="ECUC:8461c52f-b055-45ff-bac2-7437fa2dff00"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>  
                <v:var name="SpiEnableHWUnitAsyncMode" type="BOOLEAN">
                  <!-- /** @remarks Implements DSPI01320 */ -->
                  <a:a name="DESC">
                    <a:v><![CDATA[ <html> 
                        Enable Spi_SetHWUnitAsyncMode() function, which allows defining distinct operation mode (POLLING or INTERRUPT) for each HWUnit. 
                        <h1>Note</h1> This feature is not required by Autosar, which defines asynchronous mode configuration at driver level only.
                        </html> ]]>
                    </a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantLinkTime</icc:v>
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                    <icc:v class="PreCompile">VariantPostBuild</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="Freescale"/>
                  <a:a name="LOWER-MULTIPLICITY" value="1"/>
                  <a:a name="UPPER-MULTIPLICITY" value="1"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="ECUC:15a14b31-c743-4262-b959-2419f813e8ac"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="ENABLE" type="XPath">
                    <a:tst expr="(../../SpiGeneral/SpiLevelDelivered ='2')"/>
                  </a:da>
                </v:var>
                <v:var name="SpiTSBModeSupport" type="BOOLEAN">
                  <!-- /** @remarks Implements DSPI08000 */ -->
                  <a:a name="DESC">
                    <a:v><![CDATA[ <html> 
                        A feature to enable user to access the Micro Second Bus functionality.
                        </html> ]]>
                    </a:v>
                  </a:a>
                <a:a name="VISIBLE" type="XPath">
                <a:tst expr="ecu:get('SpiTSBSupport')='TRUE'" />
                </a:a> 
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantLinkTime</icc:v>
                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
